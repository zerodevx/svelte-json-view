{
  "name": "@zerodevx/svelte-json-view",
  "version": "1.0.10",
  "description": "View JSON data prettily",
  "author": "Jason Lee <jason@zerodevx.com>",
  "scripts": {
    "dev": "vite dev",
    "build:js": "vite build -c vite.dist.config.js",
    "build:copy": "cd dist/dist && cp index.umd.js index.umd.cjs",
    "package": "npm run lint && svelte-package && npm run build:js && npm run build:copy && npx publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint && npm run check",
    "test": "playwright test"
  },
  "peerDependencies": {
    "svelte": "^3.57.0 || ^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.46.1",
    "@sveltejs/adapter-auto": "^3.2.4",
    "@sveltejs/kit": "^2.5.24",
    "@sveltejs/package": "^2.3.4",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.43.0",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "terser": "^5.31.6",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./dist": {
      "import": "./dist/dist/index.es.js",
      "require": "./dist/dist/index.umd.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "jsdelivr": "./dist/dist/index.umd.js",
  "files": [
    "dist"
  ],
  "license": "ISC",
  "repository": "github:zerodevx/svelte-json-view",
  "keywords": [
    "svelte",
    "vanillajs",
    "json-pretty-print",
    "json-tree"
  ]
}
