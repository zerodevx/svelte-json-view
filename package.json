{
  "name": "@zerodevx/svelte-json-view",
  "version": "1.0.6",
  "description": "View JSON data prettily",
  "author": "Jason Lee <jason@zerodevx.com>",
  "scripts": {
    "dev": "vite dev",
    "build": "zx build.mjs",
    "prepublishOnly": "echo \"Error: publish from package/ dir instead\" && exit 1",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "format": "prettier --plugin-search-dir . --write .",
    "lint": "prettier --plugin-search-dir . --check . && npm run check && eslint .",
    "test": "playwright test"
  },
  "peerDependencies": {
    "svelte": "^3.55.1"
  },
  "devDependencies": {
    "@playwright/test": "1.35.1",
    "@sveltejs/adapter-auto": "2.1.0",
    "@sveltejs/kit": "1.20.4",
    "@sveltejs/package": "2.0.2",
    "@sveltejs/vite-plugin-svelte": "2.4.1",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-svelte3": "4.0.0",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "2.10.1",
    "svelte-check": "3.4.3",
    "terser": "5.18.0",
    "tslib": "2.5.3",
    "typescript": "5.1.3",
    "vite": "4.3.9"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "svelte": "./index.js"
    },
    "./dist": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "svelte": "./index.js",
  "jsdelivr": "./dist/index.umd.js",
  "files": [
    "**/*"
  ],
  "license": "ISC",
  "repository": "github:zerodevx/svelte-json-view",
  "keywords": [
    "svelte",
    "vanillajs",
    "json-pretty-print",
    "json-tree"
  ]
}
